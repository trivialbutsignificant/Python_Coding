   "source": [
    "#Conditional Probability\n",
    "A=float(input(\"Enter the probability of event A and B: \"))\n",
    "B=float(input(\"Enter the probability of event B: \"))\n",
    "if B!=0 and 0<=A<=1 and 0<B<=1:\n",
    "    Ans=float(A/B);\n",
    "else:\n",
    "    Ans=\"ERROR\"\n",
    "print(\"P(A|B) is :\",Ans)"
   ]
