   "source": [
    "#Sum of Distinct Numbers\n",
    "n=str(input(\"Enter all the numbers: \")).split()\n",
    "n=list(set(n))\n",
    "l=len(n)\n",
    "Sum=0\n",
    "for i in range(l):\n",
    "    n[i]=int(n[i])\n",
    "    Sum=Sum+n[i]\n",
    "print(\"Sum is\",Sum)"
   ]
