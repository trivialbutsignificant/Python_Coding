    "#Bubble Sort\n",
    "import string\n",
    "st=list(input(\"Enter the string of numbers: \"))\n",
    "l=len(st)\n",
    "print(l)\n",
    "for i in range(l-1):\n",
    "    for j in range(l-i-1):\n",
    "        if st[l-j-1]<st[l-j-2]:\n",
    "            key=st[l-j-1]\n",
    "            st[l-j-1]=st[l-j-2]\n",
    "            st[l-j-2]=key\n",
    "print(st)"
   ]
