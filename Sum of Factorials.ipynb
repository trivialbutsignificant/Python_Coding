   "source": [
    "#Finding sum of all Factorial upto n\n",
    "import math\n",
    "n=int(input(\"Enter the value of n: \"))\n",
    "Ans=1\n",
    "for i in range(1,n+1):\n",
    "    Ans=Ans+math.factorial(i)\n",
    "print(Ans)"
   ]
