   "source": [
    "#Sum of all interior angles of a regular polygon\n",
    "n=int(input(\"Enter the number of sides: \"))\n",
    "Sum=180*(n-2)\n",
    "if n>2:\n",
    "    print(\"Sum of all interior angles of the polygon is \",Sum,chr(176),sep='')\n",
    "else:\n",
    "    print(\"Invalid polygon\")"
   ]
