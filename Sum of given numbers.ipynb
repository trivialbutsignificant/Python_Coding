    "#Program to print the sum of given numbers\n",
    "n=str(input(\"Enter the numbers : \")).split()\n",
    "l=len(n)\n",
    "for i in range(l):\n",
    "    n[i]=float(n[i])\n",
    "Ans=sum(n)\n",
    "print(\"Sum is\",Ans)"
