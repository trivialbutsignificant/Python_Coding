   "source": [
    "#Program to find number of 9's in the given range\n",
    "val= input(\"Enter your value: \")\n",
    "k=0\n",
    "l=len(val)\n",
    "val=int(val)\n",
    "for i in range(val):\n",
    "    for j in range(l):\n",
    "        if ((i+1)%10**(j+1))//(10**j)==9 and len(str(i+1))>=j+1:\n",
    "            k=k+1\n",
    "print(\"Number of 9's from 1 to\",val,\"are\",k)"
   ]
  }
