   "source": [
    "#Program to print sorted list containing distinct numbers in decending order\n",
    "n=str(input(\"Enter the numbers : \")).split()\n",
    "l=len(n)\n",
    "for i in range(l):\n",
    "    n[i]=float(n[i])\n",
    "Ans=list(set(n))\n",
    "Ans.sort(reverse=1)\n",
    "print(\"Sorted list is\",Ans)"
   ]
