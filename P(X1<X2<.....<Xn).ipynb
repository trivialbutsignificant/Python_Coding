   "source": [
    "#Probability of independent random variables\n",
    "n=int(input(\"Enter the number of independent random variables: \"))\n",
    "Ans=1\n",
    "for i in range(n):\n",
    "    Ans=Ans*(i+1)\n",
    "if n!=1:\n",
    "    Ans=float(1/Ans);\n",
    "print(\"Answer P(X1<X2<.....<Xn) is :\",Ans)"
   ]
